generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Video {
  id             String   @id               @default(uuid())
  title          String
  description    String
  uploadedBy     String
  videoLink      String
  category       String
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
}